<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <!-- GENERAL -->

        <!-- eBay channel -->
        <record id="channel_ebay" model="queue.job.channel">
            <field name="name">ebay</field>
            <field name="parent_id" ref="queue_job.channel_root"/>
        </record>

        <!-- eBay sales team to crm -->
        <record id="ebay_crm_team" model='crm.team'>
            <field name="name">eBay Sales</field>
            <field name="active">true</field>
            <field name="use_quotations">true</field>
        </record>

    </data>
</odoo>